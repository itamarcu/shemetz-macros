{
  "name": "shemetz-macros",
  "title": "Shemetz's Macros",
  "description": "shem's big pile o' macros, plus some basic module code to tie them together nicely",
  "author": "shem",
  "version": "0.9.7",
  "minimumCoreVersion": "9",
  "compatibleCoreVersion": "9",
  "esmodules": [
    "scripts/shemetz-macros.js",
    "scripts/sockets.js",
    "scripts/hooks.js"
  ],
  "socket": true,
  "styles": [
    "styles/shemetz-macros.css"
  ],
  "packs": [
    {
      "name": "shemetz-macros",
      "label": "shemetz-macros",
      "module": "shemetz-macros",
      "path": "./packs/shemetz-macros.db",
      "type": "Macro"
    },
    {
      "name": "critical-hits",
      "label": "critical-hits",
      "module": "shemetz-macros",
      "path": "./packs/critical-hits.db",
      "type": "RollTable"
    }
  ],
  "dependencies": [
    {
      "name": "lib-wrapper"
    }
  ],
  "url": "https://github.com/itamarcu/shemetz-macros",
  "manifest": "https://github.com/itamarcu/shemetz-macros/raw/master/module.json",
  "download": "https://github.com/itamarcu/shemetz-macros/archive/master.zip",
  "readme": "https://github.com/itamarcu/shemetz-macros/blob/master/Readme.md",
  "bugs": "https://github.com/itamarcu/shemetz-macros/issues",
  "license": "https://github.com/itamarcu/shemetz-macros/blob/master/License.txt"
}
